

{
  "compilerOptions": {
    "outDir": "./dist",
    "module": "NodeNext",
    "target": "ES6",
    "jsx": "react-jsx",
    "moduleResolution":"NodeNext",
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src"],
  "exclude": ["node_modules", "dist"]
}